{
    "compilerOptions": {
        "target": "es5",
        "declaration": true,
        "module": "commonjs",
        "strict": true,
        "jsx": "preserve",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "sourceMap": true,
        "lib": ["esnext", "dom", "dom.iterable"]
    }
}
